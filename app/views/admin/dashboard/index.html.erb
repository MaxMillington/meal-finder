<div class="container">
  <ol class="breadcrumb">
    <li class="active">All Registered Phone Numbers</li>
  </ol>
  <h3 class="text-center">All Registered Phone Numbers</h3>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ul class="nav nav-pills" role="tablist">
        <li role="presentation">
        </li>
      </ul>
    </div>
  </div> <!-- /row -->
  <br>
  <div class="table-responsive">
    <table class="table table-hover">
      <thead>
      <tr>
        <th>ID</th>
        <th>Phone Number</th>
        <th>Email</th>
      </tr>
      </thead>
      <tbody>
      <% @numbers.each do |number| %>
          <tr>
            <td>
              <%= number.id %>
            </td>
            <td><%= number.phone_number %></td>
            <td><%= number.email %></td>
            <td><%#= link_to "Delete", #, method: :delete %></td>
          </tr>
      <% end %>
      </tbody>
    </table>

  </div>
</div>
<br>
<br>
<div style="text-align: center">
  <a class="btn btn-primary btn-lg" href="#">Twilio Test Button</a>
</div>